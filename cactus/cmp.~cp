#include <cstdlib>
#include <iostream>

using namespace std;
int n;
char st[1010];
int main(int argc, char *argv[])
{
    n = 100;
    for(int i = 1;i<=n;i++)
    {
     system("data");
     sprintf(st,"cp cactus.in cactus.in%d",i);
     system(st);
    }
    for(int i = 1;i<=n;i++)
    {
     sprintf(st,"cp cactus.in%d cactus.in",i);
     system(st);
     system("Ans2");
     system("Ans3");
     system("fc cactus.out cactus.ans2");
     system("del cactus.in");
     system("del cactus.out");
     system("del cactus.ans2");
    }
    system("PAUSE");
    return EXIT_SUCCESS;
}
